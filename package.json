{
  "name": "openg",
  "version": "2.0.0",
  "description": "open Github repo pages for npm modules in your browser",
  "license": "MIT",
  "homepage": "https://github.com/dawsonbotsford/openg#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dawsonbotsford/openg.git"
  },
  "author": {
    "name": "Dawson Botsford",
    "email": "DawsonBotsford@gmail.com",
    "url": "dawsonbotsford.com"
  },
  "bugs": {
    "url": "https://github.com/dawsonbotsford/openg/issues"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "build": "npm run babel && npm run minify",
    "babel": "babel src/index.js --out-file dist/index.js",
    "watch": "babel src/index.js --out-file dist/index.js --watch",
    "minify": "uglifyjs dist/index.js -o dist/index.min.js",
    "precommit": "npm test && npm run build",
    "test": "xo && ava"
  },
  "files": [
    "dist/index.min.js"
  ],
  "keywords": [
    "opn",
    "open",
    "browser",
    "util",
    "utils",
    "utilities",
    "github",
    "efficiency",
    "cli-app",
    "cli",
    "lean",
    "minimal",
    "documented",
    "tested",
    "licensed",
    "ava",
    "dawson"
  ],
  "dependencies": {
    "npm-name": "^3.0.0",
    "opn": "^4.0.2",
    "package-json": "^2.3.2"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel": "^5.0.0",
    "babel-cli": "^6.10.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "husky": "^0.11.4",
    "uglify-js": "^2.6.2",
    "xo": "^0.14.0"
  },
  "xo": {
    "space": true
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
